{
  "name": "hospital",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-rc.0",
    "dotenv": "^8.0.0",
    "electron-is-dev": "^1.1.0",
    "knex": "^0.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "sqlite3": "^4.0.8"
  },
  "main": "electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "npx electron electron.js",
    "preelectron-pack": "npm run build",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js",
    "electron:dev": "npx concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "electron-builder",
    "release": "npm run build && electron-builder --publish=always",
    "build:all": "npm run build && npm run electron:build",
    "seed": "node main/scripts/seed.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^7.2.4",
    "electron-builder": "^20.40.2",
    "electron-rebuild": "^1.8.5",
    "electron-reload": "^1.4.0",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "com.example.electron-hospital-idms",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
